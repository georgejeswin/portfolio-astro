---
import Algolia from "/algolia.svg";
import AWS from "/aws.svg";
import DigialOcean from "/digital-ocean.svg";
import Graphql from "/graphql.svg";
import Jira from "/jira.svg";
import NextJS from "/nextjs.svg";
import NestJs from "/nestjs.svg";
import Postgres from "/postgresql.svg";
import Trello from "/trello.svg";
import TS from "/typescript.svg";
import VSCode from "/vs-code.svg";
import Vercel from "/vercel.svg";
import Svelte from "/svelte.svg";
import Antd from "/antd.svg";
---

<section
  class="p-5 flex items-center justify-center flex-col sm:m-5 lg:m-[100px] mt-5"
>
  <h3 class="font-mono text-3xl mb-5 font-bold mt-5">Languages and Tools</h3>
  <div class="flex flex-wrap items-center justify-center">
    <a href="https://www.w3.org/html/" target="_blank" class="m-5">
      <img
        src="https://raw.githubusercontent.com/devicons/devicon/master/icons/html5/html5-original-wordmark.svg"
        alt="html5"
        width="50"
        height="50"
      />
    </a>
    <a href="https://www.w3schools.com/css/" target="_blank" class="m-5">
      <img
        src="https://raw.githubusercontent.com/devicons/devicon/master/icons/css3/css3-original-wordmark.svg"
        alt="css3"
        width="50"
        height="50"
      /><a
        href="https://developer.mozilla.org/en-US/docs/Web/JavaScript"
        target="_blank"
        class="m-5"
      >
        <img
          src="https://raw.githubusercontent.com/devicons/devicon/master/icons/javascript/javascript-original.svg"
          alt="javascript"
          width="50"
          height="50"
        />
      </a>
    </a>
    <a href="" class="m-5">
      <img src={TS} alt="" width="50px" />
    </a>
    <a href="https://www.python.org" target="_blank" class="m-5">
      <img
        src="https://raw.githubusercontent.com/devicons/devicon/master/icons/python/python-original.svg"
